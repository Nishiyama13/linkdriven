generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model jobs {
  id     Int       @id @default(autoincrement())
  title  String    @db.VarChar
  salary Decimal   @db.Decimal
  skills String[]  @db.VarChar
  until  DateTime? @db.Timestamp(6)
}
